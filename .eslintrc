{
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },

    "globals": {

    },

    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },

    "plugins": [
        "react", "jsx-a11y", "import"
    ],

    "extends": ["airbnb", "plugin:flowtype/recommended"]


}