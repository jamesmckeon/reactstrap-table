{
    "name": "@jbmckeon/reactstrap-table",
    "version": "1.0.5",
    "license": "MIT",
    "author": {
        "name": "James McKeon",
        "url": "https://github.com/jamesmckeon"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jamesmckeon/reactstrap-table.git"
    },
    "scripts": {
        "build": "webpack --config webpack.config.babel.js",
        "release": "webpack --p --config webpack.config.babel.js",
        "test": "jest",
        "coverage": "jest --coverage",
        "lint": "eslint ./src ./tests",
        "flow": "flow",
        "flow status": "flow status",
        "flow start": "flow start"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-free-solid": "^5.0.13",
        "@fortawesome/react-fontawesome": "0.0.19",
        "moment": "^2.22.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "reactstrap": "^6.0.1",
        "reactstrap-pager": "^1.0.1"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.1",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.1.2",
        "clean-webpack-plugin": "^0.1.16",
        "css-loader": "^0.28.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.9.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-flowtype": "^2.46.3",
        "eslint-plugin-flowtype-errors": "^3.5.1",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.8.2",
        "flow-bin": "^0.72.0",
        "flow-inlinestyle": "^1.0.9",
        "jest": "^22.4.4",
        "prettier-eslint": "^8.8.1",
        "react-hot-loader": "next",
        "sinon": "^5.0.7",
        "style-loader": "^0.19.0",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.4.5"
    },
    "jest": {
        "modulePaths": [
            "./src"
        ],
        "testMatch": [
            "**/tests/*.tests.js"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ]
    }
}
